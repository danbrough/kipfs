headers = libkipfsgo.h defs.h

linkerOpts =  -lkipfsgo -lssl -lcrypto

linkerOpts.linux_x64 = -Lgolib/build/golib/linuxX64 -Lopenssl/lib/linuxX64/lib
linkerOpts.linux_arm64 = -Lgolib/build/golib/linuxArm64 -Lopenssl/lib/linuxArm64/lib
linkerOpts.linux_arm32_hfp = -Lgolib/build/goib/linuxArm32Hfp -Lopenssl/lib/linuxArm32Hfp/lib
linkerOpts.macos_x64 = -Lgolib/build/golib/macosX64 -Lopenssl/lib/macosX64/lib
linkerOpts.macos_arm64 = -Lgolib/build/golib/macosArm64 -Lopenssl/lib/macosArm64/lib
linkerOpts.mingw_x64 = -Lgolib/build/golib/mingwX64 -Lopenssl/lib/mingwX64/lib
compilerOpts.mingw_x64 = -pthread